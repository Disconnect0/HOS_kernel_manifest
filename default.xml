    <project path="private/msm-google-modules/wlan" name="android_kernel_msm-modules_wlan" groups="partner" remote="hentai" />
    <project path="private/msm-google-modules/touch/fts" name="kernel/msm-modules/fts_touch" groups="partner" remote="aosp" />
